<?xml version="1.0" encoding="utf-8"?>

<resources>
    <!-- The configurable application name -->
    <string name="feature_app_name">Open eBooks</string>
    <!-- The configurable application icon -->
    <drawable name="feature_app_icon">@drawable/oei</drawable>
    <!-- The configurable splash screen image -->
    <drawable name="feature_app_splash">@drawable/oei_splash</drawable>
    <!-- The configurable main color, used for branding. -->
    <color name="feature_main_color">#8cc63e</color>
    <!-- The configurable secondary color, used for branding. -->
    <color name="feature_secondary_color">#7baf38</color>
    <!-- The default Adobe vendor ID -->
    <string name="feature_adobe_vendor_id">OEI</string>
</resources>